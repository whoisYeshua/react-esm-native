{
  "name": "react-true-esm",
  "version": "0.0.1",
  "description": "Check ESM opportunities (draft)",
  "private": true,
  "scripts": {
    "start": "npm run codegen && npm run build",
    "build": "npm run clean && npm run build:babel",
    "build:babel": "npx babel src --watch --out-dir dist --copy-files --extensions '.ts,.tsx'",
    "build:swc": "npx swc src --watch --out-dir dist --copy-files",
    "clean": "npx rimraf dist",
    "codegen": "graphql-codegen --config graphql.config.json"
  },
  "author": "whoisYeshua",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@emotion/css": "^11.10.6",
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.44",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "graphql": "^16.6.0",
    "graphql-request": "^5.2.0",
    "rimraf": "^4.4.1",
    "typescript": "^4.9.5"
  }
}
