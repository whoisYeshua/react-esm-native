{
  "name": "react-esm-native",
  "version": "1.0.0",
  "description": "Check modern ESM native opportunities in small React web application",
  "private": true,
  "scripts": {
    "start": "npm run codegen && npm run build",
    "build": "npm run clean && npm run build:babel",
    "build:babel": "npx babel src --watch --out-dir dist --copy-files --extensions '.ts,.tsx'",
    "build:swc": "npx swc src --watch --out-dir dist --copy-files",
    "clean": "npx rimraf dist",
    "codegen": "graphql-codegen --config graphql.config.json",
    "format": "npm run format:prettier",
    "format:prettier": "npm run lint:prettier -- --write",
    "lint": "npm run lint:prettier && npm run lint:typescript",
    "lint:prettier": "prettier \"**/*.{json,md,js,ts,[jt]sx,[cm][jt]s}\" --check",
    "lint:typescript": "tsc --noEmit",
    "prepare": "husky install"
  },
  "author": "whoisYeshua",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@emotion/css": "^11.10.6",
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.44",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "graphql": "^16.6.0",
    "graphql-request": "^5.2.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16.13",
    "npm": ">= 8.1"
  }
}
